{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "globals": {
    "jQuery": true,
    "$": true
  },
  "rules": {
    "indent": 2,
    "space-before-function-paren": [2, "never"],
    "valid-jsdoc": 0,
    "require-jsdoc": 0,
    "max-len": [1, 100, 4, {
      "ignoreComments": true,
      "ignoreUrls": true
    }]
  }
}
