{
    "name": "runroom/drupal_archetype",
    "license": "proprietary",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^7.1",
        "composer/installers": "1.5.0",
        "cweagans/composer-patches": "1.6.4",
        "drupal/adminimal_theme": "1.3.0",
        "drupal/console": "1.8.0",
        "drupal/core": "8.5.1",
        "drupal/devel": "1.2.0",
        "drupal/drush_language": "1.0-rc1",
        "drupal/eu_cookie_compliance": "1.0.0-rc1",
        "drupal/field_group": "1.0.0",
        "drupal/metatag": "1.4.0",
        "drupal/paragraphs": "1.2.0",
        "drupal/pathauto": "1.1.0",
        "drupal/redirect": "1.1.0",
        "drupal/twig_tweak": "1.9.0",
        "drupal/xmlsitemap": "1.0-alpha2",
        "drush/drush": "9.2.1",
        "webflo/drupal-finder": "1.1.0",
        "webmozart/path-util": "2.3.0"
    },
    "require-dev": {
        "deployer/dist": "4.3.1",
        "webflo/drupal-core-require-dev": "8.5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
