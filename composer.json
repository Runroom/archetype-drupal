{
    "name": "drupal-composer/drupal-project",
    "description": "HI-PARTNERS Drupal 8 project by RUNROOM manage with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "jqueryvalidate",
                "version": "1.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://jqueryvalidation.org/files/jquery-validation-1.14.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1",
        "composer/installers": "^1.4",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/adminimal_theme": "^1.3",
        "drupal/better_formats": "1.x-dev",
        "drupal/block_visibility_groups": "^1.1",
        "drupal/clientside_validation": "1.0-rc3",
        "drupal/console": "^1.0",
        "drupal/contact_storage": "1.0-beta9",
        "drupal/core": "^8.0",
        "drupal/eu_cookie_compliance": "1.0-beta7",
        "drupal/field_group": "^1.0",
        "drupal/geolocation": "1.10",
        "drupal/menu_block": "^1.4",
        "drupal/metatag": "^1.2",
        "drupal/minifyjs": "^1.5",
        "drupal/paragraphs": "^1.1",
        "drupal/path_redirect_import": "1.0-beta1",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "1.0-beta1",
        "drupal/responsive_favicons": "^1.4",
        "drupal/responsive_tables_filter": "1.1",
        "drupal/sitemap": "^1.3",
        "drupal/tmgmt": "1.2",
        "drupal/twig_tweak": "^1.8",
        "drupal/viewsreference": "1.0",
        "drupal/xmlsitemap": "1.0-alpha2",
        "drupal/youtube": "^1.0@beta",
        "drush/drush": "^8.0",
        "webflo/drupal-finder": "^0.3",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/devel": "1.0-rc2",
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "jcalderonzumba/gastonjs": "^1.0",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3",
        "mikey179/vfsstream": "^1.2",
        "phpunit/phpunit": "^4.8",
        "symfony/css-selector": "^2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/metatag": {
                "Attempt to create a base field bundle override of field Metatags without an entity_type": "https://www.drupal.org/files/issues/metatag-n2899752-3.patch"
            },
            "drupal/better_formats": {
                "Settings have no affect on the field if it is in a Paragraph bundle": "https://www.drupal.org/files/issues/11887466-fix-for-tmgmt-module-1.patch"
            }
        }
    }
}
