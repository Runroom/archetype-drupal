{
    "name": "drupal-composer/drupal-project",
    "description": "HI-PARTNERS Drupal 8 project by RUNROOM manage with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^7.1",
        "composer/installers": "1.5.0",
        "cweagans/composer-patches": "1.6.4",
        "drupal-composer/drupal-scaffold": "2.4.0",
        "drupal/adminimal_theme": "1.3.0",
        "drupal/console": "1.8.0",
        "drupal/core": "8.5.0",
        "drupal/eu_cookie_compliance": "1.0-beta12",
        "drupal/field_group": "1.0.0",
        "drupal/metatag": "1.4.0",
        "drupal/paragraphs": "1.2.0",
        "drupal/pathauto": "1.1.0",
        "drupal/redirect": "1.1.0",
        "drupal/twig_tweak": "1.9.0",
        "drupal/xmlsitemap": "1.0-alpha2",
        "drush/drush": "9.2.1",
        "webflo/drupal-finder": "1.1.0",
        "webmozart/path-util": "2.3.0"
    },
    "require-dev": {
        "drupal/devel": "1.0-rc2",
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "jcalderonzumba/gastonjs": "^1.0",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3",
        "mikey179/vfsstream": "^1.2",
        "phpunit/phpunit": "^4.8",
        "symfony/css-selector": "^2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/metatag": {
                "Attempt to create a base field bundle override of field Metatags without an entity_type": "https://www.drupal.org/files/issues/metatag-n2899752-3.patch"
            },
            "drupal/better_formats": {
                "Settings have no affect on the field if it is in a Paragraph bundle": "https://www.drupal.org/files/issues/11887466-fix-for-tmgmt-module-1.patch"
            }
        }
    }
}
