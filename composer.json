{
    "name": "runroom/archetype-drupal",
    "description": "This is our Drupal project template",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Runroom",
            "email": "runroom@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/Runroom/archetype-drupal",
    "require": {
        "php": "^8.1",
        "composer/installers": "2.2.0",
        "cweagans/composer-patches": "1.7.2",
        "drupal/allowed_formats": "1.5.0",
        "drupal/clientside_validation": "4.0.0",
        "drupal/config_split": "2.0.0-rc4",
        "drupal/content_snapshot": "2.0.0-beta4",
        "drupal/core-recommended": "9.4.8",
        "drupal/date_popup": "1.2.0",
        "drupal/dis": "1.0.0",
        "drupal/drush_language": "1.0.0-rc4",
        "drupal/editor_advanced_link": "2.1.1",
        "drupal/entity_reference_revisions": "1.10.0",
        "drupal/field_group": "3.4.0",
        "drupal/gin": "3.0.0-beta5",
        "drupal/gin_toolbar": "1.0.0-beta22",
        "drupal/hreflang": "1.9.0",
        "drupal/imageapi_optimize": "4.0.0",
        "drupal/mailsystem": "4.4.0",
        "drupal/menu_breadcrumb": "1.16.0",
        "drupal/menu_link_attributes": "1.2.0",
        "drupal/metatag": "1.22.0",
        "drupal/monolog": "2.2.0",
        "drupal/paragraphs": "1.15.0",
        "drupal/pathauto": "1.11.0",
        "drupal/r4032login": "2.2.0",
        "drupal/redirect": "1.8.0",
        "drupal/redis": "1.6.0",
        "drupal/swiftmailer": "2.2.0",
        "drupal/twig_field_value": "2.0.1",
        "drupal/twig_tweak": "3.2.0",
        "drupal/vbo_export": "4.1.2",
        "drupal/views_bulk_operations": "4.2.1",
        "drupal/xmlsitemap": "1.4.0",
        "drush/drush": "11.3.2",
        "symfony/dotenv": "6.0.5",
        "symfony/flex": "2.2.3"
    },
    "require-dev": {
        "behat/mink": "1.10.0",
        "deployer/deployer": "7.0.2",
        "drupal/devel": "5.0.2",
        "drupal/devel_entity_updates": "4.0.0",
        "drupal/features": "3.12.0",
        "ergebnis/composer-normalize": "2.28.3",
        "friendsofphp/php-cs-fixer": "3.4.0",
        "kint-php/kint": "4.2.3",
        "kint-php/kint-twig": "4.0",
        "mglaman/phpstan-drupal": "1.1.25",
        "palantirnet/drupal-rector": "0.13.1",
        "phpspec/prophecy-phpunit": "2.0.1",
        "phpstan/phpstan": "1.9.1",
        "phpstan/phpstan-strict-rules": "1.4.4",
        "phpunit/phpunit": "9.5.26",
        "rector/rector": "0.13.10",
        "symfony/phpunit-bridge": "6.1.6"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true
        },
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "Error running config import \"Call to a member function delete() on null in Drupal\\Core\\Config\\ConfigImporter->checkOp()\" - 3198715": "https://www.drupal.org/files/issues/2021-10-27/error-running-config-import-delete-on-null-3198715-8.patch",
                "Radios element missing \"required\" attribut - 2951317": "https://www.drupal.org/files/issues/2019-07-25/2951317-38.patch"
            }
        }
    },
    "scripts": {
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "rector": "@php rector --no-progress-bar"
    }
}
