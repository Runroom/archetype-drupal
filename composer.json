{
    "name": "runroom/drupal_archetype",
    "license": "proprietary",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^7.3",
        "composer/installers": "1.6.0",
        "cweagans/composer-patches": "1.6.6",
        "drupal/adminimal_theme": "1.5.0",
        "drupal/allowed_formats": "1.1.0",
        "drupal/config_split": "1.4.0",
        "drupal/console": "1.9.0",
        "drupal/core": "8.7.3",
        "drupal/devel": "2.1.0",
        "drupal/drush_language": "1.0.0-rc2",
        "drupal/editor_advanced_link": "1.4.0",
        "drupal/entity_reference_revisions": "1.6.0",
        "drupal/eu_cookie_compliance": "1.5.0",
        "drupal/field_group": "3.0.0-rc1",
        "drupal/hreflang": "1.1.0",
        "drupal/imageapi_optimize": "2.0.0-alpha4",
        "drupal/menu_breadcrumb": "1.9.0",
        "drupal/menu_link_attributes": "1.0.0",
        "drupal/metatag": "1.8.0",
        "drupal/paragraphs": "1.8.0",
        "drupal/pathauto": "1.4.0",
        "drupal/redirect": "1.3.0",
        "drupal/twig_field_value": "1.2.0",
        "drupal/twig_tweak": "2.3.0",
        "drupal/xmlsitemap": "1.0.0-alpha3",
        "drush/drush": "9.7.0",
        "webflo/drupal-finder": "1.1.0",
        "webmozart/path-util": "2.3.0"
    },
    "require-dev": {
        "deployer/dist": "4.3.4",
        "friendsofphp/php-cs-fixer": "2.15.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"]
        }
    }
}
