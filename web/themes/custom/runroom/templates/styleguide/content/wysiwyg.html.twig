    <h3 class="sg-section__subtitle">List Types</h3>
    <h4>Unordered List</h4>
    <div class="u-wysiwyg">
        <ul>
            {% for item in 1.. 3 %}
            <li>List Item {{ item }}
            {% endfor %}
                <ul>
                    <li>List Item 3.1.
                        <ul>
                            <li>List Item 3.1.1</li>
                        </ul>
                    </li>
                </ul>
            </li>
        </ul>
    </div>
    <h4>Ordered List</h4>
    <div class="u-wysiwyg">
        <ol>
            {% for item in 1.. 3 %}
            <li>List Item {{ item }}
            {% endfor %}
                <ol>
                    <li>List Item 3.1.
                        <ol>
                            <li>List Item 3.1.1</li>
                        </ol>
                    </li>
                </ol>
            </li>
        </ol>
    </div>
